{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    // 对象属性必须字母顺序
    "object-literal-sort-keys": false,
    // import必须字母顺序
    "ordered-imports": false,
    // 不能打console
    "no-console": [false],
    // class内默认public
    "member-access": [true, "no-public"],
    // jsx布尔值可不设值
    "jsx-boolean-value": false
	}
}
